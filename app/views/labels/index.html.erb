<%= render 'layouts/action_bar'%>
<table class="zebra-striped bordered-table super-condensed-table green">
	<thead>
	  <tr>
	    <th>Name</th>
	    <th>Type</th>
	    <th>Description</th>
	    <th>Uploaded to DFP?</th>
	    <th>Actions</th>
	  </tr>
	</thead>
	<tbody>
		<% @labels.each do |label| %>
		  <tr>
		    <td><%= label.name %></td>
		    <td><%= label.label_type %></td>
		    <td><%= label.description %></td>
			<td><%= label.dfp_id.blank? ? "No" : "Yes" %></td>	
		    <td><%= link_to 'Details', label_path %>
		  </tr>
		<% end %>
	</tbody>
</table>